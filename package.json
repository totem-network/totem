  "scripts": {
    "build": "npm run clean && webpack --config webpack.production.js",
    "clean": "rimraf build/app",
    "ethereum": "ganache-cli -m \"exclude virus file basket confirm sock ten guide actress sketch dizzy race\" -f http://localhost:10545",
    "ethereum:persist": "ganache-cli --db data/chain -m \"exclude virus file basket confirm sock ten guide actress sketch dizzy race\" -p 10545 -i 10545",
    "ipfs": "jsipfs daemon",
    "license": "license-checker",
    "serve": "webpack-dev-server --https --config webpack.development.js",
    "solium": "solium -d contracts/ || true",
    "test": "NODE_PATH=./app/src mocha --require babel-register --require ts-node/register --recursive test/app/**/*.test.ts || true",
    "test:contracts": "solium -d contracts/ && truffle test",
    "test:functional": "NODE_PATH=./app/src mocha --require babel-register --require ts-node/register --require babel-polyfill --recursive test/app/functional/**/*.test.ts || true",
    "test:integration": "gulp test:integration || true && rimraf test/build",
    "test:unit": "gulp test:unit || true && rimraf test/build",
    "webextension:build:chrome": "webextension-toolbox build chrome",
    "webextension:build:firefox": "webextension-toolbox build firefox",
    "webextension:dev:chrome": "webextension-toolbox dev chrome",
    "webextension:dev:firefox": "webextension-toolbox dev firefox",
    "zos": "zos",
    "zos:push:development": "zos push --network developmentPersist",
    "zos:update:development": "zos update Token --network developmentPersist",
    "zos:create:development": "zos create Token --init initialize --args 0x06627a2287bf6976f10132cc704e847aaf7a411a --network developmentPersist",
    "zos:push:ropsten": "zos push --network ropsten",
    "zos:create:ropsten": "zos create Token --init initialize --args 0x06180aa9A1BF60F7DB34AC3Bf37D8b1A215c8B9e --network ropsten",
    "zos:push:mainnet": "zos push --network mainnet",
    "zos:create:mainnet": "zos create Token --init initialize --args 0x9d5F5b6A34fB401F777739ba5e0F01d9165588Ea --network mainnet"
  },
