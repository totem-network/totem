  "scripts": {
    "build": "npm run build:web && npm run build:electron",
    "build:web": "npm run clean && cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config webpack.production.js",
    "build:electron": "gulp electron:build",
    "clean": "rimraf build/app",
    "ethereum": "ganache-cli -m \"exclude virus file basket confirm sock ten guide actress sketch dizzy race\"",
    "ipfs": "jsipfs daemon",
    "serve:web": "webpack-dev-server --config webpack.development.js --https",
    "serve:web:production": "webpack-dev-server --config webpack.production.js --https",
    "serve:electron": "gulp electron:serve",
    "test": "NODE_PATH=./app/src mocha --require babel-register --require ts-node/register --recursive tests/app/**/*.test.ts || true",
    "test:functional": "NODE_PATH=./app/src mocha --require babel-register --require ts-node/register --require babel-polyfill --recursive tests/app/functional/**/*.test.ts || true",
    "test:integration": "cross-env NODE_OPTIONS=--max_old_space_size=8192 gulp test:integration || true && rimraf tests/build",
    "test:unit": "cross-env NODE_OPTIONS=--max_old_space_size=8192 gulp test:unit || true && rimraf tests/build",
    "webextension:build:chrome": "webextension-toolbox build chrome",
    "webextension:build:firefox": "webextension-toolbox build firefox",
    "webextension:dev:chrome": "webextension-toolbox dev chrome",
    "webextension:dev:firefox": "webextension-toolbox dev firefox"
  },
